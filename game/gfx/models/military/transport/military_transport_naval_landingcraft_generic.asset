entity = {
	name = "military_transport_naval_landingcraft_generic_entity"
	pdxmesh = "military_transport_naval_landingcraft_generic_mesh"

	locator = { name = "locator_root" position = { 0.0 0.02 0.0 } }

	cull_radius = 10

	default_state = "moving"
	state = {
		name = "moving"
		animation = "moving_animation"
		event = { time = 0 node = "locator_root" particle = "ships/naval_landingcraft_moving" trigger_once = yes keep_particle = yes keep_sound = yes sound  = { soundeffect = "event:/SFX/Animation/vehicles/naval/light_ship/destroyer/anm_destroyer_ship_move_animation" }}

	}
	state = {
		name = "still"
		animation = "still_animation"
		event = { time = 0 node = "locator_root" particle = "ships/naval_landingcraft_idle" trigger_once = yes keep_particle = yes }
		event = { time = 0.0 keep_sound = yes sound  = { soundeffect = "event:/SFX/Animation/vehicles/naval/light_ship/destroyer/anm_destroyer_ship_idle_animation" }}
	}
	state = {
		name = "idle"
		animation = "idle_animation"
		event = { time = 0 node = "locator_root" particle = "ships/naval_landingcraft_idle" trigger_once = yes keep_particle = yes }
		event = { time = 0.0 keep_sound = yes sound  = { soundeffect = "event:/SFX/Animation/vehicles/naval/light_ship/destroyer/anm_destroyer_ship_idle_animation" }}
	}
	state = {
		name = "open"
		animation = "open_animation"
		looping = no
		event = { time = 0 node = "locator_root" particle = "ships/naval_landingcraft_idle" trigger_once = yes keep_particle = yes }
		next_state = "still"
	}
}
