entity = {
	name = "military_transport_naval_rowboat_generic_entity" 
	pdxmesh = "military_transport_naval_rowboat_generic_mesh"

	locator = { name = "locator_root" position = { 0.0 0.02 0.0 } }

	cull_radius = 10

	default_state = "moving"
	state = {
		name = "moving"
		animation = "moving_animation"
		event = { time = 0 node = "locator_root" particle = "ships/naval_rowboat_moving" trigger_once = yes keep_particle = yes keep_sound = yes sound  = { soundeffect = "event:/SFX/Animation/vehicles/transport/naval_rowboat/anm_military_transport_naval_rowboat_generic_moving" }} 

	}
	state = {
		name = "idle"
		animation = "idle_animation"
		event = { time = 0 node = "locator_root" particle = "ships/naval_rowboat_idle" trigger_once = yes keep_particle = yes keep_sound = yes sound  = { soundeffect = "event:/SFX/Animation/vehicles/transport/naval_rowboat/anm_military_transport_naval_rowboat_generic_idle" }}
	}
	state = {
		name = "stop"
		animation = "stop_animation"
		looping = no
		event = { time = 0 node = "locator_root" particle = "ships/naval_rowboat_idle" trigger_once = yes keep_particle = yes keep_sound = yes sound  = { soundeffect = "event:/SFX/Animation/vehicles/transport/naval_rowboat/anm_military_transport_naval_rowboat_generic_stop" }}
		next_state = "still"
	}
	state = {
		name = "still"
		animation = "still_animation"
		event = { time = 0 node = "locator_root" particle = "ships/naval_rowboat_idle" trigger_once = yes keep_particle = yes }
	}
}
